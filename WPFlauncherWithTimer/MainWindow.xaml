<Window x:Class="WPFlauncherWithTimer.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPFlauncherWithTimer"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="450">
    <Grid>
        <Label x:Name="label1" Content="Lanzador de aplicaciones con timer" HorizontalAlignment="Left" Margin="31,64,0,0" VerticalAlignment="Top" FontSize="24"/>
        <Label x:Name="label_2" Content="Para abrir el lanzador de click en el boton" HorizontalAlignment="Left" Margin="53,133,0,0" VerticalAlignment="Top" FontSize="18"/>
        <Button x:Name="signature" Content="Firma" HorizontalAlignment="Left" Margin="84,235,0,0" VerticalAlignment="Top" Background="#FF3BD2AC" BorderBrush="White" FontSize="16" Foreground="White" Height="33" Width="65" Click="signature_Click"/>
        <Button x:Name="openLauncher" Content="Abrir lanzador" HorizontalAlignment="Left" Margin="222,224,0,0" VerticalAlignment="Top" Background="#FFD23B3B" BorderBrush="White" FontSize="18" Foreground="White" Height="55" Width="152" Click="openLauncher_Click"/>

    </Grid>
</Window>
